[user]
	email = remy.pottier@easyvirt.com
	name = RP
[core]
	editor = vim
[alias]
	last = log -n 1 --stat
	nlast = log -n 5 --oneline
	graph = log --graph --oneline -n 20
	logfile = log -n 5 --
	aliases = config --get-regexp ^alias
  branch = branch --sort=-
  staged = diff --name-status --cached
  resetall = clean -f -d
  untrack = update-index --assume-unchanged
  track = update-index --no-assume-unchanged
[branch]
	sort = committerdate
[init]
	defaultBranch = main
